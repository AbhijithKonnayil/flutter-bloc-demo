class Customer {
  final String id;
  final String name;
  final String phoneNumber;
  final String email;
  final String address;

  Customer(
      this.id, 
      this.name,
      this.phoneNumber,
      this.email,
      this.address);

  Customer.fromJson(Map<String, dynamic> json)
      : id = json['id'],
        name = json['name'],
        phoneNumber = json['phoneNumber'],
        email = json['email'],
        address = json['address'];

  Map<String, dynamic> toJson() =>
    {
      'id': id,
      'name': name,
      'phoneNumber' : phoneNumber,
      'email' : email,
      'address' : address
    };
}